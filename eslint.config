{
  "parserOptions": { "ecmaVersion": 6 },
  "extends": "airbnb-base",
    rules: {
    'no-shadow': 0,
   'no-param-reassign': 0,
   'eol-last': 0,
   'no-use-before-define': 0,
   'no-unused-expressions': 0,
   'no-unused-vars': 0,
   'no-undef': 0,
   'import/no-cycle': 0,
   'no-restricted-globals' : 0,
  }
}
